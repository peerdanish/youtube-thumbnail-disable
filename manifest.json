{
	"manifest_version": 3,
	"name": "YouTube Thumbnail Toggle",
	"version": "1.0",
	"description": "Toggle YouTube thumbnails on or off",
	"permissions": [
	  "storage",
	  "activeTab",
	  "scripting"
	],
	"background": {
	  "service_worker": "background.js"
	},
	"action": {
	  "default_popup": "popup.html",
	"default_icon": "icon.png"
	},
	"host_permissions": [
	  "*://*.youtube.com/*"
	],
	"content_scripts": [
	  {
		"matches": ["*://*.youtube.com/*"],
		"js": ["content.js"],
		"run_at": "document_idle"
	  }
	]
  }
  